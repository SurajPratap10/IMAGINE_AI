                  darkModeIconCollection.map((darkIcon) => {
                    if (darkIcon.innerHTML === '<i class="fa-solid fa-moon"></i>') {
                      darkIcon.innerHTML = '<i class="fa-sharp fa-solid fa-sun"></i>';
                    } else {
