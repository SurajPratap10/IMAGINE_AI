<%- include('includes/header.ejs') %>
    <section class="showcase" id="showcase">
        <form id="image-form" action="index.html">
            <h1 class="AI">SIGN UP</h1>
            <div class="form-control">
                <input type="text" id="prompt" placeholder="Enter Name" />
            </div>
            <div class="form-control">
                <input type="email" id="prompt" placeholder="Enter Email address" />
            </div>
            <div class="form-control">
                <input type="password" id="prompt" placeholder="Enter password" />
            </div>


            <div class="formBtns">
                <button type="submit" class="btn sign-in-btn" href="index.html">
                    <span>Submit</span>
                    <div class="flow"></div>
                </button>

            </div>
            <div class="get-api-text">
                <p>
                    Already have an account? <a href="/login">Signin</a>

                </p>
            </div>
        </form>
    </section>
<%- include('includes/footer.ejs') %>